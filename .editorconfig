# EditorConfig is awesome: https://editorconfig.org

# top-most EditorConfig file
root = true

[*.go]
end_of_line = lf
insert_final_newline = true
indent_style = tab
indent_size = 2
charset = utf-8

[*.lua]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
charset = utf-8
